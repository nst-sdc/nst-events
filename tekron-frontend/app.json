{
  "expo": {
    "name": "Tekron",
    "slug": "tekron",
    "version": "1.1.8",
    "orientation": "portrait",
    "icon": "./assets/images/appIcon.png",
    "scheme": "tekron",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.newtonschooloftechnology.tekron"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/appIcon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "CAMERA",
        "INTERNET",
        "VIBRATE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO"
      ],
      "package": "com.newtonschooloftechnology.tekron"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/appIcon.png",
      "bundler": "metro",
      "pwa": {
        "enabled": true,
        "offline": {
          "enabled": true
        }
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/appIcon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/appIcon.png",
          "color": "#ffffff"
        }
      ],
      "expo-secure-store",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Tekron to access your photos.",
          "savePhotosPermission": "Allow Tekron to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "92a75240-89a1-405f-bf79-73c6ac1a44cb"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
